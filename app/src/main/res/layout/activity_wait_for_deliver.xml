<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.WaitForActivity">

    <com.tl.pro.travelkit.view.custom.ViewPagerIndicator
        android:id="@+id/app_about_me_goods_state_view_pager_indicator"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y35"
        android:background="#aa2faaef"
        app:item_count="5"
        >
    </com.tl.pro.travelkit.view.custom.ViewPagerIndicator>

    <android.support.v4.view.ViewPager
        android:id="@+id/app_about_me_goods_state_view_pager"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </android.support.v4.view.ViewPager>

</LinearLayout>
